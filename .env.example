# Wallet private key (64 chars WITHOUT 0x prefix)
PK=your_private_key_here

# Polymarket API credentials
CLOB_API_KEY=your_api_key
CLOB_SECRET=your_secret
CLOB_PASS_PHRASE=your_passphrase

# Optional: Funder address (for POLY_PROXY signature type)
FUNDER=0x...

# Bot settings
BOT_ORDER_SIZE=10

# Debug mode (set to 1 to enable verbose logging)
DEBUG=0

# Telegram Bot Configuration
# Get token from @BotFather on Telegram
TELEGRAM_BOT_TOKEN=123456789:ABCdefGHIjklMNOpqrsTUVwxyz

# Comma-separated list of authorized Telegram user IDs
# Get your ID from @userinfobot on Telegram
TELEGRAM_USER_IDS=123456789,987654321

# Admin user IDs (comma-separated, can stop/restart bots)
TELEGRAM_ADMIN_ID=123456789,987654321
