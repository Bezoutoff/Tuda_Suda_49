{
  "name": "tuda-suda-49",
  "version": "1.0.0",
  "description": "BTC Updown 15m Auto-Order Bot for Polymarket",
  "main": "src/bot.ts",
  "scripts": {
    "start": "ts-node src/bot.ts",
    "build": "tsc",
    "dev": "ts-node-dev src/bot.ts",
    "test-order": "ts-node src/test-order.ts",
    "test-ws": "ts-node src/test-ws.ts",
    "bot-polling": "ts-node src/bot-polling.ts",
    "bot-fast": "node dist/bot-polling.js",
    "test-latency": "ts-node src/test-latency.ts",
    "test-latency-cpp": "ts-node src/test-latency-cpp.ts",
    "build:cpp": "bash build-cpp.sh",
    "updown-bot": "ts-node src/updown-bot-cpp/updown-bot-cpp.ts",
    "build:updown-bot": "bash build-updown-bot.sh",
    "build:all-cpp": "npm run build:cpp && npm run build:updown-bot",
    "fill-timestamps": "ts-node scripts/fill-accepting-timestamp.ts",
    "analyze-timing": "ts-node scripts/analyze-timing.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bezoutoff/Tuda_Suda_49.git"
  },
  "keywords": [
    "polymarket",
    "trading",
    "bot",
    "btc"
  ],
  "author": "Bezoutoff",
  "license": "MIT",
  "dependencies": {
    "@polymarket/clob-client": "^4.22.8",
    "@polymarket/real-time-data-client": "^1.4.0",
    "dotenv": "^17.2.3",
    "ethers": "^5.8.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
